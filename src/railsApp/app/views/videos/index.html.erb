        localTracks.play('agora_remote' + uid);
      }
      if (mediaType === 'audio') {
        user.audioTrack.play();
      }
    }


  }

  function handleUserPublished(user, mediaType) {
    const id = user.uid;
    remoteUsers[id] = user;
